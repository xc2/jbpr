{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "allowImportingTsExtensions": true,
    "lib": ["ESNext"],
    "types": ["bun-types", "node", "deno"]
  },
  "exclude": ["BUILD.*"]
}
